{
  "manifest_version": 3,
  "name": "New Word Tab",
  "description": "Learn a word per new tab",
  "version": "2.0.0",
  "chrome_url_overrides": {
    "newtab": "./dist/newTab/index.html"
  },
  "options_ui": {
    "page": "./dist/options/index.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "./assets/16.png",
    "32": "./assets/32.png",
    "48": "./assets/48.png",
    "128": "./assets/128.png"
  },
  "content_security_policy": {
    "extension_pages": "img-src * data:;object-src 'self';script-src 'self'"
  }
}
